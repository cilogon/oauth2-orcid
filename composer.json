{
    "name": "cilogon/oauth2-orcid",
    "description": "ORCID OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "NCSA",
    "authors": [
        {
            "name": "Terry Fleury",
            "email": "tfleury@cilogon.org",
            "homepage": "http://www.cilogon.org"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "orcid"
    ],
    "require": {
        "php": ">=5.6.0",
        "league/oauth2-client": "^2.6.0",
        "lcobucci/jwt": ">=3.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "mockery/mockery": "^1.4.4",
        "php-parallel-lint/php-parallel-lint": "^1.3.1",
        "squizlabs/php_codesniffer": "^3.6.1"
    },
    "autoload": {
        "psr-4": {
            "CILogon\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CILogon\\OAuth2\\Client\\Test\\": "test/src/"
        }
    }
}
